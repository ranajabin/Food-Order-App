                    </MenuButton>

                    <MenuButton text="Select" width="10rem" icon="sap-icon://menu" buttonMode="Split" defaultAction="onDefaultAction" beforeMenuOpen="onBeforeMenuOpen">
                        <menu>
                            <Menu itemSelected="onMenuAction">
                                <MenuItem text="Your orders" icon="sap-icon://edit" buttonMode="Split" defaultAction="onDefaultAction" beforeMenuOpen="onBeforeMenuOpen" press="onPress">
                                    <customData>
                                        <core:CustomData key="target" value="p1" />
                                    </customData>
                                </MenuItem>
                                <MenuItem text="Favorite orders" icon="sap-icon://save" />
                                <MenuItem text="Address book" icon="sap-icon://open-folder" />
                                <MenuItem text="More" icon="sap-icon://open-folder">
                                    <items>
                                        <MenuItem text="About" icon="" />
                                        <MenuItem text="Send feedback" icon="" />
                                        <MenuItem text="Rate us" icon="" />
                                    </items>
                                </MenuItem>
                            </Menu>
                        </menu>
                    </MenuButton>
                </HBox>

                <VBox>
                    <SearchField id="searchField" width="380px" placeholder="Search for..." enableSuggestions="true" search=".onSearch" suggest=".onSuggest" class=".sapMSFF" />
